                                   Foreground="#666666"
                                   FontSize="10"
                                   FontWeight="Bold"
                                   Margin="0 0 10 0" />

                        <Grid>
                            <StackPanel Orientation="Horizontal"
                                        Margin="5 4"
                                        HorizontalAlignment="Left">
                                <Slider Width="180"
                                        Value="100"
                                        Orientation="Horizontal"
                                        HorizontalAlignment="Center"
                                        IsSnapToTickEnabled="True"
                                        Minimum="0"
                                        Maximum="100"
                                        TickPlacement="BottomRight"
                                        AutoToolTipPlacement="BottomRight"
                                        AutoToolTipPrecision="2"
                                        ValueChanged="ValueChanged"
                                        TickFrequency="1"
                                        Cursor="Hand"
                                        Margin="-10 0 0 0"
                                        x:Name="SlOpacity" />

                                <TextBlock Text="100%"
                                           Foreground="#666666"
                                           HorizontalAlignment="Right"
                                           FontSize="12"
                                           Margin="5 0"
                                           x:Name="OpacityValue" />
                            </StackPanel>

                            <Border DockPanel.Dock="Right"
                                    Width="30"
                                    Height="30"
                                    Background="#6200EA"
                                    HorizontalAlignment="Right"
                                    x:Name="ColorBox"
                                    BorderBrush="#666666"
                                    BorderThickness="0.5" />
                        </Grid>
                    </StackPanel>

                    <Grid DockPanel.Dock="Top" Margin="0 10 0 0">
                        <StackPanel HorizontalAlignment="Left">
                            <TextBlock Text="Hexadecimal color"
                                       DockPanel.Dock="Top"
                                       Foreground="#666666"
                                       FontSize="10"
                                       FontWeight="Bold" />
                            <TextBox Text="#000000"
                                     Height="Auto"
                                     Width="108"
                                     HorizontalAlignment="Left"
                                     TextChanged="OnTextChanged"
                                     BorderBrush="#666666"
                                     BorderThickness="0 0 0 2"
                                     Foreground="#666666"
                                     FontSize="10"
                                     DockPanel.Dock="Top"
                                     Padding="0 2"
                                     FontWeight="Bold"
                                     x:Name="TbHexa" />
                        </StackPanel>

                        <StackPanel HorizontalAlignment="Right">
                            <TextBlock Text="RGB color"
                                       Foreground="#666666"
                                       FontSize="10"
                                       FontWeight="Bold"
                                       Margin="10 0 0 0" />

                            <DockPanel Margin="10 0 0 10">
                                <StackPanel Orientation="Horizontal">
                                    <Border Background="Red"
                                            Width="12"
                                            BorderThickness="1"
                                            BorderBrush="Red">
                                        <TextBlock Text="R"
                                                   Foreground="#ffffff"
                                                   FontSize="12"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center" />
                                    </Border>
                                    <TextBox Text="255"
                                             Width="24"
                                             BorderBrush="Red"
                                             BorderThickness="1"
                                             Foreground="Red"
                                             FontSize="10"
                                             Padding="0 2"
                                             HorizontalContentAlignment="Center"
                                             x:Name="TbR" />
                                </StackPanel>

                                <StackPanel Orientation="Horizontal">
                                    <Border Background="Green"
                                            Width="12"
                                            BorderThickness="1"
                                            BorderBrush="Green">
                                        <TextBlock Text="G"
                                                   Foreground="#ffffff"
                                                   FontSize="12"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center" />
                                    </Border>
                                    <TextBox Text="255"
                                             Width="24"
                                             BorderBrush="Green"
                                             BorderThickness="1"
                                             Foreground="Green"
                                             FontSize="10"
                                             Padding="0 2"
                                             HorizontalContentAlignment="Center"
                                             x:Name="TbG" />
                                </StackPanel>

                                <StackPanel Orientation="Horizontal">
                                    <Border Background="Blue"
                                            Width="12"
                                            BorderThickness="1"
                                            BorderBrush="Blue">
                                        <TextBlock Text="B"
                                                   Foreground="#ffffff"
                                                   FontSize="12"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center" />
                                    </Border>
                                    <TextBox Text="255"
                                             Width="24"
                                             BorderBrush="Blue"
                                             BorderThickness="1"
                                             Foreground="Blue"
                                             FontSize="10"
                                             Padding="0 2"
                                             HorizontalContentAlignment="Center"
                                             x:Name="TbB" />
                                </StackPanel>

                                <StackPanel Orientation="Horizontal">
                                    <Border Background="Black"
                                            Width="12"
                                            BorderThickness="1"
                                            BorderBrush="Black">
                                        <TextBlock Text="A"
                                                   Foreground="#ffffff"
                                                   FontSize="12"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center" />
                                    </Border>
                                    <TextBox Text="1"
                                             Width="24"
                                             BorderBrush="Black"
                                             BorderThickness="1"
                                             Foreground="Black"
                                             FontSize="10"
                                             Padding="0 2"
                                             HorizontalContentAlignment="Center"
                                             x:Name="TbA" />
                                </StackPanel>
                                <Grid />
                            </DockPanel>
                        </StackPanel>
                    </Grid>

                    <Border BorderBrush="#dddddd"
                            BorderThickness="0 1.4 0 0"
                            Margin="-10 5 -10 0"
                            Padding="10 5 10 0"
                            DockPanel.Dock="Top">
                        <StackPanel>
                            <TextBlock Text="Ajouter du dégradé"
                                       Foreground="#666666"
                                       FontSize="10"
                                       FontWeight="Bold" />

                            <Grid Margin="0 4 0 10">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>

                                <ComboBox Grid.Row="0"
                                          Grid.Column="0"
                                          Grid.ColumnSpan="2"
                                          BorderBrush="#8c8c8a"
                                          BorderThickness="0 0 0 2"
                                          Foreground="#8c8c8a"
                                          Height="23"
                                          VerticalAlignment="Bottom"
                                          x:Name="CbGradientType"
                                          Tag="GradientType"
                                          Text="0"
                                          FontSize="10"
                                          SelectedIndex="0">
                                    <ComboBoxItem Content="Linear Gradient" />
                                    <ComboBoxItem Content="Radial Gradient" />
                                </ComboBox>

                                <Grid Grid.Row="1"
                                      Grid.Column="0"
                                      Margin="0 10 5 5"
                                      Width="145"
                                      Height="100"
                                      x:Name="GradientOrientationPanel">
                                    <Border Background="#dddddd"
                                            Padding="0"
                                            BorderThickness="0"
                                            CornerRadius="2"
                                            Margin="6"
                                            MouseLeftButtonDown="OnChangePosition" />

                                    <Line X1="12"
                                          Y1="20"
                                          X2="132"
                                          Y2="80"
                                          StrokeThickness="1"
                                          Stroke="#6200EA"
                                          Opacity=".4"
                                          x:Name="LineOrientation">
                                    </Line>

                                    <Border Background="#ff2c2c"
                                            Padding="0"
                                            Width="14"
                                            Height="14"
                                            BorderBrush="#6200EA"
                                            BorderThickness="2"
                                            CornerRadius="10"
                                            Opacity=".4"
                                            Margin="0 10 0 0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Top"
                                            x:Name="LeftButton"
                                            Tag="LeftButton"
                                            MouseLeftButtonDown="OnChangeButton" />

                                    <Border Background="#6200EA"
                                            Padding="0"
                                            Width="14"
                                            Height="14"
                                            BorderBrush="#6200EA"
                                            BorderThickness="2"
                                            CornerRadius="10"
                                            Opacity=".4"
                                            Margin="131 75 0 0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Top"
                                            x:Name="RightButton"
                                            Tag="RightButton"
                                            MouseLeftButtonDown="OnChangeButton" />
                                </Grid>

                                <ItemsControl Grid.Row="1"
                                              Grid.Column="1"
                                              BorderThickness="0"
                                              x:Name="LvColors"
                                              Background="Transparent"
                                              Margin="5 10 0 0 "
                                              Height="100">
                                    <ItemsControl.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <WrapPanel />
                                        </ItemsPanelTemplate>
                                    </ItemsControl.ItemsPanel>
                                    <ItemsControl.ItemTemplate>
                                        <DataTemplate>
                                            <Border Width="20"
                                                    Height="20"
                                                    Background="{Binding Color}"
                                                    Padding="0"
                                                    BorderBrush="#dddddd"
                                                    BorderThickness="0.5"
                                                    VerticalAlignment="Top"
                                                    Margin="0 0 5 5"
                                                    CornerRadius="2"
                                                    d:DataContext="{d:DesignInstance }" />
                                        </DataTemplate>
                                    </ItemsControl.ItemTemplate>
                                </ItemsControl>
                            </Grid>
                        </StackPanel>
                    </Border>
                </DockPanel>
            </StackPanel>
        </Border>

        <Border Grid.Column="1"
                Grid.Row="0"
                Background="#ffffff"
                BorderBrush="#F44336"
                BorderThickness="0.4"
                MouseLeftButtonDown="OnClose"
                Tag="Close"
                Margin="-12 0 0 -10"
                Cursor="Hand"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                CornerRadius="10">
            <materialDesign:PackIcon Kind="Close"
                                     Width="15"
                                     Height="15"
                                     Margin="2"
                                     Foreground="#F44336" />
        </Border>
    </Grid>
</Window>