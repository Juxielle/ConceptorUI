<Application x:Class="ConceptorUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ConceptorUI"
             StartupUri="SplashView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="GoogleFontIcon">pack://application:,,,/Assets/Icons/MaterialIcons-Regular.ttf#Material Icons</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
